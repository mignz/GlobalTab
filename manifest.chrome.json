{
  "manifest_version": 3,
  "name": "GlobalTab",
  "description": "New tab page showing your favorite bookmarks.",
  "version": "1.2.1",
  "icons": {
    "16": "assets/img/icon_16.png",
    "48": "assets/img/icon_128.png",
    "128": "assets/img/icon_128.png"
  },
  "author": "Miguel Nunes",
  "chrome_url_overrides": {
    "newtab": "globaltab.html"
  },
  "permissions": [
    "bookmarks",
    "favicon"
  ]
}
